{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "Rabbit": {
    "UserName": "user",
    "Password": "pass",
    "VirtualHost": "/",
    "HostName": "host",
    "Exchanges": [{
      "Exchange": "exampleexchange",
      "Type": "topic",
      "Durable": false,
      "autoDelete": false
    }],
    "Queues": [{
      "Name": "examplename",
      "Exchange": "exampleexchange",
      "RoutingKey": "key.example.*",
      "Exclusive": false,
      "Durable": true,
      "AutoDelete": false
    }]
  },
  "Mongo": {
    "ConnectionString": "connectionstring",
    "Database": "databasename"
  }
}